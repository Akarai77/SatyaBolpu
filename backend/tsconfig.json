{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2020",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": true,
    "declaration": false,
    "noEmit": false,
    "outDir": "./.ts-node",
    "allowSyntheticDefaultImports": true
  }
}
